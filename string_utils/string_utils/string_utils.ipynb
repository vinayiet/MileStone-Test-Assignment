    "def reverse_string(s):\n",
    "    return s[::-1]\n",
    "\n",
    "def capitalize_string(s):\n",
    "    return s.capitalize()\n",
    "\n",
    "def uppercase_string(s):\n",
    "    return s.upper()\n",
    "\n",
    "def lowercase_string(s):\n",
    "    return s.lower()\n",
    "\n",
    "def titlecase_string(s):\n",
    "    return s.title()\n",
    "\n",
    "def is_palindrome(s):\n",
    "    return s == s[::-1]\n"
   ]
